{
  "name": "react-app-boilerplate",
  "version": "1.0.0",
  "description": "React Boilerplate code",
  "main": "index.js",
  "author": "alexander-lebed",
  "license": "UNLICENSED",
  "engines": {
    "node": ">=13.x",
    "yarn": ">=1.3.2"
  },
  "scripts": {
    "build": "webpack --progress --colors --config webpack.config.dev.js",
    "build-prod": "webpack --progress --optimize-minimize -p --config ./webpack.config.prod.js",
    "heroku-postbuild": "webpack --progress --optimize-minimize -p --config ./webpack.config.prod.js",
    "precommit": "yarn build-prod && git add .",
    "start": "cross-env NODE_ENV=production node start.js",
    "dev": "cross-env NODE_ENV=development nf start -j Procfile.dev",
    "prod": "nf start -j Procfile",
    "test": "cross-env WS_ADDRESS=ws://localhost:3000 jest --coverage",
    "lint": "eslint src",
    "lint-fix": "eslint src --fix --cache",
    "flow": "flow"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn precommit"
    }
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "express": "^4.16.3",
    "foreman": "3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-flow": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@babel/register": "^7.8.3",
    "@babel/runtime": "^7.8.4",
    "axios": "^0.21.1",
    "babel": "^6.23.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "cross-env": "^7.0.0",
    "css-loader": "^1.0.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.8.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-flowtype": "^4.6.0",
    "eslint-plugin-react": "^7.18.3",
    "file-loader": "^1.1.11",
    "flow-bin": "^0.71.0",
    "flow-typed": "^2.4.0",
    "husky": "^4.2.3",
    "jest": "^24.8.0",
    "node-sass": "^4.9.3",
    "node-sass-glob-importer": "^5.3.2",
    "nodemon": "1.18.9",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "react-test-renderer": "^16.8.6",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.22.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.35.2",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "3.7.2",
    "webpack-merge": "^4.2.2"
  }
}
